<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.nimko.trayapp.fx.windows.NotesWindows">
   <children>
      <FlowPane fx:id="pane"
        hgap="10" vgap="10"
        padding="10"
        AnchorPane.topAnchor="0"
        AnchorPane.leftAnchor="0"
        AnchorPane.rightAnchor="0"
        AnchorPane.bottomAnchor="0">
      </FlowPane>

      <Button text="+"
        fx:id="addButton"
        style="-fx-font-size: 24px; -fx-background-radius: 50%;"
        prefWidth="50" prefHeight="50"
        AnchorPane.bottomAnchor="20"
        AnchorPane.rightAnchor="20"
        />
   </children>
</AnchorPane>
